
procedure Main is
   Number : Integer := 111;
   Trial_Divisor : Integer := 2;
   Is_Prime : Boolean := True;
begin
   while Trial_Divisor < Number loop
      if Number rem Trial_Divisor = 0 then
         Is_Prime := False;
      end if;   
      Trial_Divisor := Trial_Divisor + 1;
   end loop;
end Main;
