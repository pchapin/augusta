-- This file tests expressions involving parentheses.

begin
   X: = A and (B or C);
   X := (A or B) and C;
   X := not (A and B);
   X := (A + B) * C;
   X := A / (B + C);
   X := (A > B) + C;
   X := (A < B) * C;
   X := +(A +  B);
   X := -(A * +B);

   -- Multiple levels of parentheses.
   X := ((A + B)/C + D);
   X := ((A + B)/(C + D) + E);
   X := ((A + B)/(C + D)) / -E;
end
